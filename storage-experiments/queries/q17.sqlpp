USE tpch;


WITH tmp AS
(
    SELECT l_partkey t_partkey, 0.2 * avg(l_quantity) t_avg_quantity
    FROM LineItem
    GROUP BY l_partkey
)

SELECT ELEMENT SUM(l.l_extendedprice) / 7.0
FROM  
      Part p, tmp t,
      LineItem l
WHERE p.p_partkey = t.t_partkey AND p.p_container = 'MED BOX' AND p.p_brand = 'Brand#23'
            AND t.t_partkey = l.l_partkey AND l.l_quantity < t.t_avg_quantity
;
